{
  "name": "medokvono",
  "version": "0.1.0",
  "private": true,
  "build-css": "node-sass-chokidar --include-path ./src/assets/styles --include-path ./node_modules src//assets/styles -o src/",
  "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/assets/styles --include-path ./node_modules src/assets/styles -o src/assets/styles --watch --recursive",
  "homepage": "https://vyacheslavtykhonchuk.github.io/MedKvon",
  "dependencies": {
    "axios": "^0.18.0",
    "connected-react-router": "^4.4.1",
    "gh-pages": "^1.2.0",
    "history": "^4.7.2",
    "node-sass-chokidar": "^1.3.3",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/assets/styles -o src/assets/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/styles -o src/assets/styles --watch --recursive"
  }
}
